{
    "useTabs": true,
    "singleQuote": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSameLine": true,
    "tabWidth": 4,
    "singleAttributePerLine": true,
    "printWidth": 100,
    "plugins": [ "prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
}
